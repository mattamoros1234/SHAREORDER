<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="it.unibo.paw.hibernate.Gruppo" table="gruppi">
	 	<id name="id" type="integer" column="id" >
  			<generator class="increment"/>
   		</id>
		<property name="nomeGruppo" column="nomeGruppo" />
		<property name="emailCreatore" column="emailCreatore" />
	</class>
</hibernate-mapping>