<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="it.unibo.paw.hibernate.Pacco" table="pacchi">
	 	<id name="id" type="integer" column="id" >
  			<generator class="increment"/>
   		</id>
		<property name="statoPacco" column="statoPacco" />
		<property name="emailMittente" column="emailMittente" />
		<property name="emailDestinatario" column="emailDestinatario" />
		<property name="indirizzoMittente" column="indirizzoMittente" />
		<property name="indirizzoDestinatario" column="indirizzoDestinatario" />
		<property name="emailCorriere" column="emailCorriere" />
	</class>
</hibernate-mapping>